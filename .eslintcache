   fill={active ? '#44D7B6' : ''}\n        stroke={active ? '' : 'rgba(255,255,255,0.4)'}\n        strokeWidth=\"0.658823529\"\n      ></path>\n      <path\n        d=\"M21.5867647,10.9014706 L20.1383824,10.9014706 C19.8233824,10.9014706 19.5238235,11.0527941 19.3385294,11.3122059 L14.4838235,18.0445588 L12.285,14.9933824 C12.0997059,14.7370588 11.8032353,14.5826471 11.4851471,14.5826471 L10.0367647,14.5826471 C9.83602941,14.5826471 9.71867647,14.8111765 9.83602941,14.9748529 L13.6839706,20.3113235 C14.0761765,20.8579412 14.8883824,20.8579412 15.2805882,20.3113235 L21.7844118,11.2936765 C21.9048529,11.13 21.7875,10.9014706 21.5867647,10.9014706 Z\"\n        fill=\"#FFFFFF\"\n      ></path>\n    </g>\n  </svg>\n)\n","/Users/guichizhao/shuttleflow/src/history/Histories.jsx",[],"/Users/guichizhao/shuttleflow/src/caption/Form.jsx",["214"],"import React from 'react'\nimport useStyle from '../component/useStyle'\nimport inputStyles from '../component/input.module.scss'\nimport buttonStyles from '../component/button.module.scss'\nimport formStyles from './Form.module.scss'\nimport { useTranslation } from 'react-i18next'\nimport useSWR from 'swr'\nimport { useForm } from 'react-hook-form'\nimport * as yup from 'yup'\nimport { yupResolver } from '@hookform/resolvers'\nimport { ErrorMessage } from '@hookform/error-message'\n\nimport profile from './profile.svg'\nimport useTokenList from '../data/useTokenList'\nimport MainContainer from '../component/MainContainer/MainContainer'\nimport PaddingContainer from '../component/PaddingContainer/PaddingContainer'\n\nconst __mock_balance = 10000\nexport default function CaptionForm({ token }) {\n  const { t } = useTranslation()\n  const [inputCx, buttonCx, formCx] = useStyle(\n    inputStyles,\n    buttonStyles,\n    formStyles\n  )\n\n  const { tokens } = useTokenList(token, { isReference: true })\n  console.log(tokens)\n  const tokenInfo = tokens && tokens.length > 0 ? tokens[0] : {}\n\n  //we do not need to call the contract if we know the token is not available\n  const { data: captionInfo = {} } = useSWR(\n    true ? ['/caption', 'caption_unlocked_me'] : null,\n    function () {\n      return Promise.resolve({})\n    }\n  )\n\n  // if (!tokenInfo) {\n  //   return null\n  // }\n\n  // const { data: tokenInfo } = useSWR(['/address', token], swrSearchTokenFetcher, { suspense: true })\n  //cAddress 判定跨链与否\n  const { icon, symbol, cSymbol, name, minMortgage, cAddress } = tokenInfo\n\n  const { countdown, caption, pending_mount, pending_cost } = captionInfo || {\n    countdown: 0,\n    caption: '--',\n    pending_mount: '--',\n    pending_cost: '--',\n  }\n\n  //若当前连接钱包的 Address 为该 token captain 本人时\n  //token captain 规则表单中 “抵押数量”默认值为0，\n  //下方说明文案变为“0 表示不更新抵押，\n  //或者重新竞争 captain，需要最小抵押数量 XX cETH”。\n  const isMe = caption === '0x_address_of_me'\n\n  const onSubmit = (data) => console.log(data)\n\n  const _minMortgage = Math.max(2, minMortgage ? minMortgage * 1.1 : 0)\n\n  const schema = yup.object().shape({\n    inFee: yup\n      .number()\n      .typeError(t('errors.number'))\n      .min(0, t('errors.above-zero'))\n      .test('below-in-fee', t('errors.below-in-fee'), function (params) {\n        const { parent } = this\n        return Number.isNaN(parent.inMin) || params < parent.inMin\n      }),\n    outFee: yup\n      .number()\n      .typeError(t('errors.number'))\n      .min(0, t('errors.above-zero'))\n      .test('below-in-fee', t('errors.below-out-fee'), function (params) {\n        const { parent } = this\n        return Number.isNaN(parent.outMin) || params < parent.outMin\n      }),\n    inMin: yup\n      .number()\n      .typeError(t('errors.number'))\n      .min(0, t('errors.above-zero'))\n      .test('above-in-fee', t('errors.above-in-fee'), function (params) {\n        const { parent } = this\n        return Number.isNaN(parent.inFee) || params > parent.inFee\n      }),\n    outMin: yup\n      .number()\n      .typeError(t('errors.number'))\n      .test('above-out-fee', t('errors.above-in-fee'), function (params) {\n        const { parent } = this\n        return Number.isNaN(parent.outFee) || params > parent.outFee\n      }),\n    wallet_fee: yup\n      .number()\n      .typeError(t('errors.number'))\n      .min(0, t('errors.above-zero')),\n    minMortgage: yup\n      .number()\n      //todo: not sure isMe or _minMortgage\n      //will be cached incorrectly due to closure\n      .typeError(t('errors.number'))\n      .max(__mock_balance, t('errors.insufficient'))\n      .test(\n        'above-current-ifnot-me',\n        t('errors.above-current'),\n        (v) => (isMe && v === 0) || v > _minMortgage\n      ),\n  })\n\n  const { register, handleSubmit, errors, setValue, watch } = useForm({\n    resolver: yupResolver(schema),\n    defaultValues: {\n      ...tokenInfo,\n      minMortgage: !cAddress ? 2 : isMe ? 0 : _minMortgage,\n    },\n    mode: 'onBlur',\n  })\n  const { minMortgage: minMortgageInput } = watch(['minMortgage'])\n\n  //current user is caption of the symbol and\n  //the mortgage of the symbol is not modified\n  const selfNotUpdate = isMe && minMortgageInput === '0'\n\n  const fields = [\n    {\n      label: t('label.shuttle-in-fee'),\n      unit: cSymbol,\n      name: 'inFee',\n      readOnly: cAddress && countdown !== 0,\n    },\n    {\n      label: t('label.shuttle-out-fee'),\n      unit: cSymbol,\n      name: 'outFee',\n      readOnly: cAddress && countdown !== 0,\n    },\n    {\n      label: t('label.shuttle-in-amount'),\n      unit: symbol,\n      name: 'inMin',\n      readOnly: cAddress && countdown !== 0,\n    },\n    {\n      label: t('label.shuttle-out-amount'),\n      unit: cSymbol,\n      name: 'outMin',\n      readOnly: cAddress && countdown !== 0,\n    },\n    {\n      label: t('label.create-fee'),\n      name: 'wallet_fee',\n      unit: cSymbol,\n      readOnly: cAddress && countdown !== 0,\n    },\n  ]\n\n  return (\n    <MainContainer>\n      <PaddingContainer bottom top>\n        <Head />\n        <form onSubmit={handleSubmit(onSubmit)}>\n          {fields.map((props, i) => input(props))}\n          <>\n            <div className={formCx('input-container')}>\n              <div className={formCx('label')}>{t('label.morgage-amount')}</div>\n              <input\n                ref={register}\n                name=\"minMortgage\"\n                autoComplete=\"off\"\n                data-lpignore=\"true\"\n                className={\n                  inputCx(\n                    'input-common',\n                    errors['minMortgage'] ? 'error' : ''\n                  ) +\n                  ' ' +\n                  formCx('input')\n                }\n                placeholder={t('placeholder.enter')}\n              />\n              {selfNotUpdate && (\n                <div className={formCx('for-not-update')}>\n                  {t('txt.for-not-update')}\n                </div>\n              )}\n              <div className={formCx('after')}>cETH</div>\n            </div>\n            <div className={formCx('small-text', 'bottom-text')}>\n              <div>{t('txt.min-mortgage', { minMortgage: _minMortgage })}</div>\n              <div>\n                <span>\n                  {' '}\n                  {t('txt.ceth-balance', { amount: __mock_balance })}\n                </span>\n                <span\n                  onClick={() => {\n                    setValue('minMortgage', __mock_balance)\n                  }}\n                  className={formCx('all')}\n                >\n                  {' '}\n                  {t('btn.all')}\n                </span>\n              </div>\n            </div>\n            <ErrorMessage\n              errors={errors}\n              name=\"minMortgage\"\n              render={({ message }) => {\n                return <p className={formCx('error')}>{message}</p>\n              }}\n            />\n          </>\n\n          <input\n            type=\"submit\"\n            value={selfNotUpdate ? t('btn.update') : t('btn.be-caption')}\n            className={buttonCx('btn') + ' ' + formCx('btn')}\n          />\n        </form>\n      </PaddingContainer>\n    </MainContainer>\n  )\n\n  //NOTICE:\n  //input should NOT be a component, i.e. not Input\n  //remounted Conponent can not be identified by register\n  function input({ label, name, readOnly, unit }) {\n    return (\n      <div key={label}>\n        <div className={formCx('input-container')}>\n          <div className={formCx('label')}>{label}</div>\n          <input\n            ref={register}\n            name={name}\n            autoComplete=\"off\"\n            readOnly={readOnly}\n            data-lpignore=\"true\"\n            className={\n              inputCx('input-common', errors[name] ? 'error' : '') +\n              ' ' +\n              formCx('input')\n            }\n            placeholder={t('placeholder.enter')}\n          />\n          <div className={formCx('after')}>{unit}</div>\n        </div>\n        <ErrorMessage\n          errors={errors}\n          name={name}\n          render={({ message }) => {\n            return <p className={formCx('error')}>{message}</p>\n          }}\n        />\n      </div>\n    )\n  }\n\n  function Head() {\n    return (\n      <>\n        <div className={formCx('first-container')}>\n          <div className={formCx('left')}>\n            <img className={formCx('icon')} alt=\"logo\" src={icon}></img>\n            <div className={formCx('left-text')}>\n              <div className={formCx('large-text')}>{symbol}</div>\n              <div className={formCx('small-text')}>{name}</div>\n            </div>\n          </div>\n          <div className={formCx('right')}>\n            <div className={formCx('large-text')}>\n              {(minMortgage || '--') + ' cETH'}\n            </div>\n            <div\n              className={formCx('small-text')}\n              style={{ display: 'flex', alignItems: 'center' }}\n            >\n              <img className={formCx('profile')} src={profile}></img>\n              <span>{caption}</span>\n            </div>\n          </div>\n        </div>\n        <div className={formCx('second-container')}>\n          <div className={formCx('second-item')}>\n            <div className={formCx('large-text')}>{pending_mount}</div>\n            <div className={formCx('small-text', 'mTop')}>\n              {t('txt.pending-amount')}\n            </div>\n          </div>\n          <div className={formCx('second-item')}>\n            <div className={formCx('large-text')}>{pending_cost}</div>\n            <div className={formCx('small-text', 'mTop')}>\n              {t('txt.pending-cost')}\n            </div>\n          </div>\n          <div className={formCx('second-item')}>\n            <div className={formCx('large-text')}>{formatSec(countdown)}</div>\n            <div className={formCx('small-text', 'mTop')}>\n              {t('txt.lock-down')}\n            </div>\n          </div>\n        </div>\n      </>\n    )\n  }\n}\n\nfunction formatSec(sec) {\n  let hour = parseInt(sec / 3600)\n  sec -= hour * 3600\n  const minute = parseInt(sec / 60)\n  sec -= minute * 60\n  return [hour, minute, sec].map(padZero).join(':')\n}\n\nfunction padZero(value) {\n  value += ''\n  if (value.length === 1) {\n    value = '0' + value\n  }\n  return value\n}\n","/Users/guichizhao/shuttleflow/src/shuttle/shuttle-in/ShuttleIn.jsx",[],"/Users/guichizhao/shuttleflow/src/shuttle/shuttle-out/ShuttleOut.jsx",[],"/Users/guichizhao/shuttleflow/src/data/useState1.js",[],"/Users/guichizhao/shuttleflow/src/data/tokenList.js",[],"/Users/guichizhao/shuttleflow/src/data/jsonrpc.js",[],"/Users/guichizhao/shuttleflow/src/data/formatNum.js",[],"/Users/guichizhao/shuttleflow/src/data/useShuttleInAddress.js",[],"/Users/guichizhao/shuttleflow/src/history/HistoryItem.jsx",[],"/Users/guichizhao/shuttleflow/src/shuttle/TokenInput.jsx",[],"/Users/guichizhao/shuttleflow/src/history/ShuttleHistory.jsx",[],"/Users/guichizhao/shuttleflow/src/shuttle/CTokenPopup.jsx",[],"/Users/guichizhao/shuttleflow/src/shuttle/Input.jsx",[],"/Users/guichizhao/shuttleflow/src/component/Icon/Icon.jsx",[],{"ruleId":"215","replacedBy":"216"},{"ruleId":"217","replacedBy":"218"},{"ruleId":"219","severity":1,"message":"220","line":18,"column":8,"nodeType":"221","endLine":18,"endColumn":10,"suggestions":"222"},{"ruleId":"223","severity":1,"message":"224","line":2,"column":25,"nodeType":"225","messageId":"226","endLine":2,"endColumn":29},{"ruleId":"227","severity":1,"message":"228","line":17,"column":1,"nodeType":"229","endLine":27,"endColumn":2},{"ruleId":"227","severity":1,"message":"230","line":3,"column":1,"nodeType":"229","endLine":23,"endColumn":2},{"ruleId":"231","severity":1,"message":"232","line":281,"column":15,"nodeType":"233","endLine":281,"endColumn":64},"no-native-reassign",["234"],"no-negated-in-lhs",["235"],"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'clickAway'. Either include it or remove the dependency array. If 'clickAway' changes too often, find the parent component that defines it and wrap that definition in useCallback.","ArrayExpression",["236"],"no-unused-vars","'Link' is defined but never used.","Identifier","unusedVar","import/no-anonymous-default-export","Unexpected default export of anonymous function","ExportDefaultDeclaration","Assign arrow function to a variable before exporting as module default","jsx-a11y/alt-text","img elements must have an alt prop, either with meaningful text, or an empty string for decorative images.","JSXOpeningElement","no-global-assign","no-unsafe-negation",{"desc":"237","fix":"238"},"Update the dependencies array to be: [clickAway]",{"range":"239","text":"240"},[615,617],"[clickAway]"]